
function solucao(notas) {
    let maior = 100
    let soma=0;
    let sub=0;
    let media =0;
    let menor;
    let tamanho;
    menor = Math.min.apply(Math, notas)
	for(let i =0; i<notas.length; i++){
        
        if(notas[i]>maior){
            maior = i
           
        }
        soma +=notas[i]
        sub = soma-maior-menor
        tamanho=notas.length-2
        media = sub/tamanho;
}
   
    console.log( media)
    
   
   
}

