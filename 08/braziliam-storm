
function solucao(notas) {
    
    notas.sort((a,b)=>a-b);// Ordena os elementos do array em ordem Crescente
    notas.pop(); // remove o ultimo
    notas.shift(); // remove o primeiro
    let soma =0;
    let media =0;
    for(let x=0; x<notas.length; x++){
        soma+=notas[x];
        media = soma/notas.length
    }
    console.log(media)
   
}

